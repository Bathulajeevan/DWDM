prices <- c(1,1,5,5,5,5,8,8,10,10,10,12,14,14,14,15,15,15,15,18,18,18,18,18,20,20,20,20,21,21,21,21,25,25,25,25,25,28,28,30,30)
bins <- cut(prices, breaks=3)
tapply(prices, bins, mean) # Bin means
tapply(prices, bins, range) # Bin boundaries (min/max)
hist(prices, breaks=3, main="Histogram of Prices")
